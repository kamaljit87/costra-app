    "@azure/identity": "^4.13.0",
    "@google-cloud/billing": "^5.1.1",
    "@ibm-cloud/platform-services": "^0.81.0",
    "@sentry/node": "^10.36.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "digitalocean": "^1.2.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
