{
  "name": "costra-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "node --watch server.js",
    "start": "node server.js",
    "setup": "node setup.js",
    "pm2:start": "pm2 start ../ecosystem.config.js --only costra-backend",
